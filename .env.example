# Discord Bot Configuration
DISCORD_BOT_TOKEN=your_discord_bot_token_here

# Stable Diffusion API Configuration
SD_API_URL=http://localhost:7860
SD_API_TIMEOUT=600

# Ollama LLM Configuration
OLLAMA_API_URL=http://localhost:11434
OLLAMA_MODEL=huihui_ai/gpt-oss-abliterated:20b
# OLLAMA_MODEL=huihui_ai/qwen3-abliterated:0.6b

# Gemini API Configuration
GEMINI_API_KEY=your_gemini_api_key_here

# xAI API Configuration
XAI_API_KEY=your_xai_api_key_here

# Database Configuration
DATABASE_URL=sqlite+aiosqlite:///./data/database.db

# Storage Configuration
IMAGE_STORAGE_PATH=./data/images

# Application Configuration
LOG_LEVEL=INFO
ENVIRONMENT=development

# Default Generation Settings
DEFAULT_IMAGE_COUNT=4
DEFAULT_WIDTH=512
DEFAULT_HEIGHT=512
DEFAULT_STEPS=20
DEFAULT_CFG_SCALE=11.0
DEFAULT_SAMPLER=DDIM CFG++
DEFAULT_SCHEDULER=DDIM
DEFAULT_MODEL=sdxl
# DEFAULT_MODEL=waiNSFWIllustrious_v150

# Web Research (Optional)
GOOGLE_SEARCH_API_KEY=
GOOGLE_SEARCH_ENGINE_ID=

# SD WebUI Stub Configuration (Optional)
# スタブサーバーからの通知を受け取るDiscordチャンネルID
# 未設定の場合はDiscord通知なしでスタブが動作します
STUB_DISCORD_CHANNEL_ID=
